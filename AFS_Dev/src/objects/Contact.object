    <validationRules>
        <fullName>Account_Cannot_Be_Intel</fullName>
        <active>true</active>
        <description>Contacts cannot be linked to Intel accounts.</description>
        <errorConditionFormula>OR
((AND (TEXT(Account.Type) = &apos;Sub Portfolio&apos;, Account.Name=  &apos;Intel&apos;)),
Account.Sub_Portfolio__c = &apos;Intel&apos;)</errorConditionFormula>
        <errorDisplayField>AccountId</errorDisplayField>
        <errorMessage>CON-001: Intel Contacts should not be listed in SMaRT. Please select a non-Intel account.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Invalid_Assistant_Phone</fullName>
        <active>true</active>
        <description>This validation accepts number only else it will throw error</description>
        <errorConditionFormula>AND(
NOT(ISBLANK(AssistantPhone )),
NOT(REGEX(AssistantPhone ,&quot;[0-9()+ -]+&quot;))
)</errorConditionFormula>
        <errorDisplayField>AssistantPhone</errorDisplayField>
        <errorMessage>CON-002-005 Please enter a valid phone number.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Invalid_Fax_Number</fullName>
        <active>true</active>
        <description>User cannot input a letter to this field</description>
        <errorConditionFormula>AND(
NOT(ISBLANK(Fax )),
NOT(REGEX(Fax ,&quot;[0-9()+ -]+&quot;))
)</errorConditionFormula>
        <errorDisplayField>Fax</errorDisplayField>
        <errorMessage>CON-002-004 Please enter a valid phone number.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Invalid_Mobile_Phone</fullName>
        <active>true</active>
        <description>User cannot input a letter to this field</description>
        <errorConditionFormula>AND(
NOT(ISBLANK(MobilePhone)),
NOT(REGEX(MobilePhone,&quot;[0-9()+ -]+&quot;))
)</errorConditionFormula>
        <errorDisplayField>MobilePhone</errorDisplayField>
        <errorMessage>CON-002-002 Please enter a valid phone number.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Invalid_Other_Phone</fullName>
        <active>true</active>
        <description>User cannot input a letter in this field</description>
        <errorConditionFormula>AND(
NOT(ISBLANK(OtherPhone )),
NOT(REGEX(OtherPhone ,&quot;[0-9()+ -]+&quot;))
)</errorConditionFormula>
        <errorDisplayField>OtherPhone</errorDisplayField>
        <errorMessage>CON-002-003 Please enter a valid phone number.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Invalid_Phone</fullName>
        <active>true</active>
        <description>User cannot input a letter in this field</description>
        <errorConditionFormula>AND(
NOT(ISBLANK(Phone)),
NOT(REGEX(Phone,&quot;[0-9()+ -]+&quot;))
)</errorConditionFormula>
        <errorDisplayField>Phone</errorDisplayField>
        <errorMessage>CON-002-001 Please enter a valid phone number.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Invalid_Phone_Numbers</fullName>
        <active>true</active>
        <description>Show error message if user inputs invalid phone numbers</description>
        <errorConditionFormula>OR(
	AND(
	NOT(ISBLANK(AssistantPhone )),
	NOT(REGEX(AssistantPhone ,&quot;[0-9()+ -]+&quot;))
	),
	AND(
	NOT(ISBLANK(Fax )),
	NOT(REGEX(Fax ,&quot;[0-9()+ -]+&quot;))
	),
	AND(
	NOT(ISBLANK(MobilePhone)),
	NOT(REGEX(MobilePhone,&quot;[0-9()+ -]+&quot;))
	),
	AND(
	NOT(ISBLANK(OtherPhone )),
	NOT(REGEX(OtherPhone ,&quot;[0-9()+ -]+&quot;))
	),
	AND(
	NOT(ISBLANK(Phone)),
	NOT(REGEX(Phone,&quot;[0-9()+ -]+&quot;))
	)
)</errorConditionFormula>
        <errorMessage>CON-002: Please enter a valid phone number.</errorMessage>
    </validationRules>
