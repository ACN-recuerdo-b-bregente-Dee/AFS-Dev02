        <description>Checks required field(s) in stage 0 if null and returns an error message to fill up required field(s).</description>
        <errorConditionFormula>AND (
ISPICKVAL( Status_Stage_Requested__c , &quot;0&quot;)
,
OR (
ISBLANK(Name),
ISBLANK(TEXT(StageName)),
ISBLANK(TEXT(Type)), 
ISBLANK(AccountId), 
ISBLANK(CloseDate),
ISBLANK(Probability)
)
)</errorConditionFormula>
        <errorMessage>OPP-040: Fields required to enter Stage 0. Review all error messages below to correct your data.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Required_Fields_Validation_Rule_Stage_1A</fullName>
        <active>true</active>
        <description>Checks required field(s) in stage 1A if null and returns an error message to fill up required field(s).</description>
        <errorConditionFormula>AND (
ISPICKVAL( Status_Stage_Requested__c , &quot;1A&quot;)
,
OR (
ISBLANK(TEXT(Best_Probable_Worst__c)),
ISBLANK(Client_Industry_Area_of_Service__c),
ISBLANK(Complexity_Category__c ), ISBLANK(TEXT(Hierarchy_Level__c)),
ISBLANK(TEXT( Industry_Sub_Segment__c )),
ISBLANK(TEXT( Primary_Function__c )), 
ISBLANK(TEXT(Is_AFS_Prime_or_Subcontractor__c)), 
ISBLANK(Proposal_Submission_Date__c ), 
ISBLANK(TEXT(The_Opportunity_involves_activities_now__c)),
ISBLANK(TEXT(ONCONUS_Work_Required__c)),
ISBLANK( TEXT(Export_Transfer_of_Products_Software__c)),
ISBLANK(Subject_countries_client_operates__c), 
ISBLANK(TEXT(Win_Probability__c)),

AND(
ISBLANK(RFP_Release_Date__c ),
OR (ISPICKVAL( Type ,&quot;Competitive&quot;),
ISPICKVAL( Type ,&quot;Re-Compete&quot;))
) 
)
)</errorConditionFormula>
        <errorMessage>OPP-041: Fields required to enter Stage 1A. Review all error messages below to correct your data.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Required_Fields_Validation_Rule_Stage_2A</fullName>
        <active>true</active>
        <description>Checks required field(s) in stage 2A if null and returns an error message to fill up required field(s).</description>
        <errorConditionFormula>AND ( 
ISPICKVAL( Status_Stage_Requested__c , &quot;2A&quot;) 
,ISBLANK( Solicitation_Contract_Number__c ) 
)</errorConditionFormula>
