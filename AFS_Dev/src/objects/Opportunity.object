        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/connection.js&quot;)} 

// identify the record 
var opp = new sforce.SObject(&quot;Opportunity&quot;); 
opp.id = &quot;{!Opportunity.Id}&quot;; 
if(&apos;{!User.Profile}&apos; != &apos;System Administrator&apos; &amp;&amp; &apos;{!User.Profile}&apos; != &apos;AFS Sales Ops&apos;)
{

window.location = &apos;/apex/Admin_Only&apos;

} 
else
{

// make the field change 
 opp.Send_Email_to_OSP__c = true; 
 opp.Generate_User_Email__c = &apos;{!User.Email}&apos;;

// save the change 
 var result = sforce.connection.update([opp]); 
 
   if ( result[0].getBoolean( &quot;success&quot; ) ) {

    location.reload( true ); // refresh page

	} else {

    var errors = result[0].errors;
    var errorMessages = &quot;&quot;;

    for ( var i = 0; i &lt; errors.length; i++ ) {
      errorMessages += errors[i].message + &apos;\n&apos;;
    }

    alert( &apos;The current opportunity contains data errors and unable to send the Offline Approval email until errors are corrected&apos; ); // display all validation errors

  }

//refresh the page 
 //window.location.reload();
}</url>
