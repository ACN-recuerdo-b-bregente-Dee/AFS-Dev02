        <errorConditionFormula>AND(ISPICKVAL(QA_been_documented_and_finalized_in_QA__c,&quot;No&quot;),ISBLANK(QA_Not_Documented_and_Finalized_in_QA_To__c))</errorConditionFormula>
        <errorDisplayField>QA_Not_Documented_and_Finalized_in_QA_To__c</errorDisplayField>
