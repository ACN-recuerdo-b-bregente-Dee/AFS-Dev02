    <validationRules>
        <fullName>ATS_Notes_is_required_at_Stage_2B</fullName>
        <active>true</active>
        <description>ATS Notes is required to enter Stage 2B</description>
        <errorConditionFormula>AND (
OR (
ISPICKVAL( Status_Stage_Requested__c , &quot;2B&quot;),
ISPICKVAL( Status_Stage_Requested__c , &quot;3&quot;)
),
ISBLANK (ATS_Notes__c)
)</errorConditionFormula>
        <errorDisplayField>ATS_Notes__c</errorDisplayField>
        <errorMessage>OPP-043-009: Required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Account_Pl_required_Stage_2B_Beyond</fullName>
        <active>true</active>
        <description>Account Plan OI% is required to enter Stage 2B</description>
        <errorConditionFormula>AND (
OR (
ISPICKVAL( Status_Stage_Requested__c , &quot;2B&quot;),
ISPICKVAL( Status_Stage_Requested__c , &quot;3&quot;)
),
 ISPICKVAL(Account_OI_Plan_Be_Met__c , &quot;&quot;)
)</errorConditionFormula>
        <errorDisplayField>Account_OI_Plan_Be_Met__c</errorDisplayField>
        <errorMessage>OPP-043-003: Required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Actual_Contract_Sign_Date_before_dates</fullName>
        <active>true</active>
        <description>The Actual Contract Sign Date validation rule: This date field should never come before the Draft RFP Release Date, RFP Release Date, and Proposal Submission Date.</description>
        <errorConditionFormula>OR(OR( Actual_Contract_Sign_Date__c &lt;=   Draft_RFP_Release_Date__c  ,Actual_Contract_Sign_Date__c &lt;=  RFP_Release_Date__c ),Actual_Contract_Sign_Date__c &lt;=  Proposal_Submission_Date__c)</errorConditionFormula>
        <errorMessage>OPP-022: Please enter an Actual Contract Sign Date after the Draft RFP Release Date, RFP Release Date, and Proposal Submission Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Bank_Guarantee_Letter_of_Credit_is_req</fullName>
        <active>true</active>
        <description>Bank Guarantee/Letter of Credit is required to enter Stage 2A</description>
        <errorConditionFormula>OR 
( 
AND ( 
ISBLANK( TEXT( Bank_Guarantee_Letter_of_Credit__c )), 
OR ( 
ISPICKVAL( Status_Stage_Requested__c , &quot;2A&quot;), 
ISPICKVAL( Status_Stage_Requested__c , &quot;2B&quot;), 
ISPICKVAL( Status_Stage_Requested__c , &quot;3&quot;) 
), 
TEXT( Source__c) &lt;&gt; &quot;STaR&quot; 
), 
AND ( 
ISBLANK( TEXT(Bank_Guarantee_Letter_of_Credit__c )), 
TEXT( Source__c) = &quot;STaR&quot;, 
OR ( 
ISPICKVAL( Status_Stage_Requested__c , &quot;2A&quot;), 
ISPICKVAL( Status_Stage_Requested__c , &quot;2B&quot;), 
ISPICKVAL( Status_Stage_Requested__c , &quot;3&quot;) 
), 
OR( 
ISPICKVAL( PRIORVALUE(Status_Stage_Requested__c) , &quot;0&quot;), 
ISPICKVAL( PRIORVALUE(Status_Stage_Requested__c) , &quot;1A&quot;), 
ISPICKVAL( PRIORVALUE(Status_Stage_Requested__c) , &quot;1B&quot;) 
) 
) 
)</errorConditionFormula>
        <errorDisplayField>Bank_Guarantee_Letter_of_Credit__c</errorDisplayField>
        <errorMessage>OPP-042-002: Required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Best_Probable_Worst_is_required</fullName>
        <active>true</active>
        <description>Best/Probable/Worst is required to enter Stage 1A</description>
        <errorConditionFormula>AND (
OR (
ISPICKVAL( Status_Stage_Requested__c , &quot;1A&quot;),
ISPICKVAL( Status_Stage_Requested__c , &quot;1B&quot;),
ISPICKVAL( Status_Stage_Requested__c , &quot;2A&quot;),
ISPICKVAL( Status_Stage_Requested__c , &quot;2B&quot;),
ISPICKVAL( Status_Stage_Requested__c , &quot;3&quot;)
),
ISBLANK(TEXT(Best_Probable_Worst__c))
)</errorConditionFormula>
        <errorDisplayField>Best_Probable_Worst__c</errorDisplayField>
        <errorMessage>OPP-041-002: Required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_modify_Reporting_Status_Since</fullName>
        <active>true</active>
        <errorConditionFormula>ISCHANGED( Reporting_Status_Since__c ) &amp;&amp; NOT(OR($Profile.Name = &quot;AFS Sales Ops&quot; , 
    $Profile.Name = &quot;System Administrator&quot; ))</errorConditionFormula>
        <errorDisplayField>Reporting_Status_Since__c</errorDisplayField>
        <errorMessage>OPP-004: Only AFS Sales Ops users can change Reporting Status Since Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_modify_Stage_Field_Value</fullName>
        <active>true</active>
        <description>OPP-006</description>
        <errorConditionFormula>AND(
    ISCHANGED( StageName ),
    AND($Profile.Name &lt;&gt; &quot;System Administrator&quot;,   
    $Profile.Name &lt;&gt; &quot;AFS System Admin&quot;)
)</errorConditionFormula>
        <errorDisplayField>StageName</errorDisplayField>
        <errorMessage>OPP-006: Post Opportunity creation, this field is system updated only.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Client_Data_Prot_is_required_at_Stage_2B</fullName>
        <active>true</active>
        <description>Client Data Protection Risk Assessment is required to enter Stage 2B</description>
        <errorConditionFormula>OR
(
AND (
ISBLANK( TEXT(Client_Data_Protection_Risk_Assessment__c)),
OR (
ISPICKVAL( Status_Stage_Requested__c , &quot;2B&quot;),
ISPICKVAL( Status_Stage_Requested__c , &quot;3&quot;)
),
TEXT( Source__c) &lt;&gt; &quot;STaR&quot;
),
AND (
ISBLANK( TEXT(Client_Data_Protection_Risk_Assessment__c)),
TEXT( Source__c) = &quot;STaR&quot;,
OR (
ISPICKVAL( Status_Stage_Requested__c , &quot;2B&quot;),
ISPICKVAL( Status_Stage_Requested__c , &quot;3&quot;)
),
OR(
ISPICKVAL(  PRIORVALUE(Status_Stage_Requested__c) , &quot;0&quot;),
ISPICKVAL(  PRIORVALUE(Status_Stage_Requested__c) , &quot;1A&quot;),
ISPICKVAL(  PRIORVALUE(Status_Stage_Requested__c) , &quot;1B&quot;),
ISPICKVAL(  PRIORVALUE(Status_Stage_Requested__c) , &quot;2A&quot;)
)
)
)</errorConditionFormula>
        <errorDisplayField>Client_Data_Protection_Risk_Assessment__c</errorDisplayField>
        <errorMessage>OPP-043-011: Required</errorMessage>
    </validationRules>
