        <fullName>QA_3_fields_required_TDP_Greater3M</fullName>
        <active>true</active>
        <description>You have entered a Total  Deal Potential greater than $3M. Please enter the Date of Opportunity QA Review 3, QA Risk Tier 3 and QA Assessment 3 fields in order to enter Stage 3.</description>
        <errorConditionFormula>AND(
AND(TEXT(Type) &lt;&gt; &quot;Extension&quot;, TEXT(Type) &lt;&gt; &quot;Option&quot;, 
Total_Deal_Potential__c &gt;= 3000) 
,
ISPICKVAL( Status_Stage_Requested__c , &quot;3&quot;)
,
OR(
ISBLANK( TEXT(QA_Assessment_Stage_3__c ) ),
ISBLANK( TEXT(Date_of_Opportunity_QA_Review_3__c ) ),
ISBLANK( TEXT(QA_Risk_Tier_Stage_3__c ) )
)
)</errorConditionFormula>
        <errorMessage>OPP-036: You have entered a TDP greater than $3M. Please enter the Date of Opportunity QA Review 3, QA Risk Tier 3 and QA Assessment 3 fields in order to enter Stage 3.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>QA_Assessment_3_is_required</fullName>
        <active>true</active>
        <description>You have entered a Total  Deal Potential greater than $1M and less than or equal to $3M. Please enter the Date of Opportunity QA Review 3, QA Risk Tier 3 and QA Assessment 3 fields in order to enter Stage 3.</description>
        <errorConditionFormula>AND(

AND(TEXT(Type) &lt;&gt; &quot;Extension&quot;, TEXT(Type) &lt;&gt; &quot;Option&quot;, 
Total_Deal_Potential__c &gt; 1000, Total_Deal_Potential__c &lt;= 3000)
,
ISPICKVAL( Status_Stage_Requested__c , &quot;3&quot;)
,
OR(
ISBLANK( TEXT(QA_Assessment_Stage_3__c ) ),
ISBLANK( TEXT(Date_of_Opportunity_QA_Review_3__c ) ),
ISBLANK( TEXT(QA_Risk_Tier_Stage_3__c ) )
)
)</errorConditionFormula>
        <errorMessage>OPP-034: You have entered a TDP greater than $1M and less than or equal to $3M. Please enter the Date of Opportunity QA Review 3, QA Risk Tier 3 and QA Assessment 3 fields in order to enter Stage 3.</errorMessage>
    </validationRules>
    <validationRules>
