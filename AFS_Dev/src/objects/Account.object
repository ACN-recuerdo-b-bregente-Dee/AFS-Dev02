    <validationRules>
        <fullName>Cannot_Update_Inactive_Accounts</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISPICKVAL( Status__c ,&quot;Inactive&quot;),
   OR(
	ISCHANGED(Account_Plan_OI__c),
	ISCHANGED(Client_Account_Lead__c),
	ISCHANGED(Client_Classification__c),
	ISCHANGED(Client_QA_Director__c),
	ISCHANGED(CostPoint_Invoice_Customer__c),
	ISCHANGED(CostPoint_Invoice_Customer_ID__c),
	ISCHANGED(P_Global_Client_Account_Lead__c),
	ISCHANGED(Managing_Director_MC__c),
	ISCHANGED(Operations_Lead__c),
	ISCHANGED(Portfolio__c),
	ISCHANGED(Sub_Portfolio__c),
	ISCHANGED(Technology_Account_Lead__c),
	ISCHANGED( ParentId ),
	ISCHANGED(Description),
	ISCHANGED(Website),
	ISCHANGED(Type),
	ISCHANGED( OwnerId )
   )
)</errorConditionFormula>
        <errorMessage>ACC-002: Unable to update inactive Account. Please change Account status to active to make updates.</errorMessage>
